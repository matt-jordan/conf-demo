{
    "ecmaFeatures": {
    },

    "rules": {
        "comma-dangle": [1, "never"],
        "no-cond-assign": [2, "except-parens"],
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 1,

        "accessor-pairs": 0,
        "block-scoped-var": 1,
        "complexity": [1, 5],
        "consistent-return": 1,
        "curly": 2,
        "default-case": 2,
        "dot-notation": [1, {"allowKeywords": true, "allowPattern": "^[a-z]+(_[a-z]+)+$"}],
        "dot-location": [1, "property"],
        "eqeqeq": 1,
        "guard-for-in": 1,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 0,
        "no-else-return": 1,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 1,
        "no-implied-eval": 2,
        "no-iterator": 0,
        "no-labels": 0,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new-func": 2,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 1,
        "no-octal": 1,
        "no-param-reassign": 0,
        "no-process-env": 0,
        "no-proto": 1,
        "no-redeclare": 2,
        "no-return-assign": 1,
        "no-script-url": 0,
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unused-expressions": 0,
        "no-void": 1,
        "no-warning-comments": 1,
        "no-with": 2,
        "radix": 1,
        "vars-on-top": 0,
        "wrap-iife": [2, "inside"],
        "yoda": 1,

        "strict": [2, "global"],

        "no-catch-shadow": 0,
        "no-delete-var": 1,
        "no-label-var": 1,
        "no-shadow-restricted-names": 1,
        "no-shadow": 1,
        "no-undef-init": 1,
        "no-undef": 1,
        "no-undefined": 1,
        "no-unused-vars": 1,
        "no-use-before-define": 0,

        "indent": [2, 2],
        "quotes": [1, "single", "avoid-escape"],
        "semi": 2
    },

    "env": {
        "node": true,
        "mocha": true
    }
}
